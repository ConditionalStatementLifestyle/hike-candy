<%= form_for :user, url: '/signup' do |f| %>
  <%=f.label :first_name%>
  <%=f.text_field :first_name%>
  <%=f.label :last_name%>
  <%=f.text_field :last_name%><br>
  <%=f.label :username%>
  <%=f.text_field :username%><br><br>
  <%=f.label :password%>
  <%=f.password_field :password%><br>
  <%=f.label "Confirm Password"%>
  <%=f.password_field :password_confirmation%><br>
  <%=f.submit "Create"%>
<%end%>
